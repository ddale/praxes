####### Compiler, tools and options

CC       = gcc
CXX      = g++
LEX      = flex
YACC     = yacc
CFLAGS   = -pipe -Wall -W -O2 -g -pipe -march=i386 -mcpu=i686 -fno-use-cxa-atexit -fno-exceptions  -DQT_THREAD_SUPPORT -DQT_NO_DEBUG
CXXFLAGS = -pipe -Wall -W -O2 -g -pipe -march=i386 -mcpu=i686 -fno-use-cxa-atexit -fno-exceptions  -DQT_THREAD_SUPPORT -DQT_NO_DEBUG
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = -I$(QTDIR)/mkspecs/default -I. -I$(QTDIR)/include -I.ui/ -I. -I.moc/
LINK     = g++
LFLAGS   = 
LIBS     = $(SUBLIBS) -L$(QTDIR)/lib -L/usr/X11R6/lib -lqt-mt -ljpeg -lImlib -lXv ../libxvidcore.a -lqt-mt -lXext -lX11 -lm
AR       = ar cqs
RANLIB   = 
MOC      = $(QTDIR)/bin/moc
UIC      = $(QTDIR)/bin/uic
QMAKE    = qmake
TAR      = tar -cf
GZIP     = gzip -9f
COPY     = cp -f
COPY_FILE= $(COPY) -p
COPY_DIR = $(COPY) -pR
DEL_FILE = rm -f
SYMLINK  = ln -sf
DEL_DIR  = rmdir
MOVE     = mv -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p

####### Output directory

OBJECTS_DIR = .obj/

####### Files

HEADERS = video.h \
		videorenderer.h \
		../datasource.h
SOURCES = main.cpp \
		video.c \
		videorenderer.cpp \
		../datasource.cpp
OBJECTS = .obj/main.o \
		.obj/video.o \
		.obj/videorenderer.o \
		.obj/datasource.o \
		.obj/videowidget.o
FORMS = videowidget.ui
UICDECLS = .ui/videowidget.h
UICIMPLS = .ui/videowidget.cpp
SRCMOC   = .moc/moc_videowidget.cpp
OBJMOC = .obj/moc_videowidget.o
DIST	   = videowidget.pro
QMAKE_TARGET = videowidget
DESTDIR  = 
TARGET   = videowidget

first: all
####### Implicit rules

.SUFFIXES: .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: videowidget.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile videowidget.pro
qmake: 
	@$(QMAKE) -o Makefile videowidget.pro

dist: 
	@mkdir -p .obj/videowidget && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/videowidget/ && $(COPY_FILE) --parents videowidget.ui.h .obj/videowidget/ && ( cd `dirname .obj/videowidget` && $(TAR) videowidget.tar videowidget && $(GZIP) videowidget.tar ) && $(MOVE) `dirname .obj/videowidget`/videowidget.tar.gz . && $(DEL_FILE) -r .obj/videowidget

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS) 
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

.obj/main.o: main.cpp .ui/videowidget.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.obj/video.o: video.c xvid.h
	$(CC) -c $(CFLAGS) $(INCPATH) -o .obj/video.o video.c

.obj/videorenderer.o: videorenderer.cpp videorenderer.h \
		../datasource.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/videorenderer.o videorenderer.cpp

.obj/datasource.o: ../datasource.cpp ../datasource.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/datasource.o ../datasource.cpp

.ui/videowidget.h: videowidget.ui videorenderer.h \
		../datasource.h
	$(UIC) videowidget.ui -o .ui/videowidget.h

.ui/videowidget.cpp: .ui/videowidget.h videowidget.ui videorenderer.h \
		../datasource.h
	$(UIC) videowidget.ui -i videowidget.h -o .ui/videowidget.cpp

.obj/videowidget.o: .ui/videowidget.cpp videowidget.ui.h \
		.ui/videowidget.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/videowidget.o .ui/videowidget.cpp

.obj/moc_videowidget.o: .moc/moc_videowidget.cpp .ui/videowidget.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_videowidget.o .moc/moc_videowidget.cpp

.moc/moc_videowidget.cpp: $(MOC) .ui/videowidget.h
	$(MOC) .ui/videowidget.h -o .moc/moc_videowidget.cpp

####### Install

install: all 

uninstall: 

