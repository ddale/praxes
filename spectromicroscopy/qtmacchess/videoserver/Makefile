####### Compiler, tools and options

CC       = gcc
CXX      = g++
LEX      = flex
YACC     = yacc
CFLAGS   = -pipe -Wall -W -O2 -g -pipe -march=i386 -mcpu=i686  -DQT_THREAD_SUPPORT -DQT_NO_DEBUG -DQT_SHARED
CXXFLAGS = -pipe -Wall -W -O2 -g -pipe -march=i386 -mcpu=i686  -DQT_THREAD_SUPPORT -DQT_NO_DEBUG -DQT_SHARED
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = -I/usr/lib/qt-3.3/mkspecs/default -I. -I$(QTDIR)/include -I.ui/ -I. -I.moc/
LINK     = g++
LFLAGS   = 
LIBS     = $(SUBLIBS) -L$(QTDIR)/lib -L/usr/X11R6/lib -lXv /usr/local/lib/libxvidcore.a -lraw1394 -ldc1394_control -lqt-mt -lXext -lX11 -lm
AR       = ar cqs
RANLIB   = 
MOC      = $(QTDIR)/bin/moc
UIC      = $(QTDIR)/bin/uic
QMAKE    = qmake
TAR      = tar -cf
GZIP     = gzip -9f
COPY     = cp -f
COPY_FILE= $(COPY)
COPY_DIR = $(COPY) -r
INSTALL_FILE= $(COPY_FILE)
INSTALL_DIR = $(COPY_DIR)
DEL_FILE = rm -f
SYMLINK  = ln -sf
DEL_DIR  = rmdir
MOVE     = mv -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p

####### Output directory

OBJECTS_DIR = .obj/

####### Files

HEADERS = video.h \
		driver.h \
		videorenderer.h \
		videoserver.h \
		clientconnection.h
SOURCES = main.cpp \
		video.c \
		driver.c \
		videorenderer.cpp \
		videoserver.cpp \
		clientconnection.cpp
OBJECTS = .obj/main.o \
		.obj/video.o \
		.obj/driver.o \
		.obj/videorenderer.o \
		.obj/videoserver.o \
		.obj/clientconnection.o \
		.obj/videowidget.o
FORMS = videowidget.ui
UICDECLS = .ui/videowidget.h
UICIMPLS = .ui/videowidget.cpp
SRCMOC   = .moc/moc_clientconnection.cpp \
		.moc/moc_videowidget.cpp
OBJMOC = .obj/moc_clientconnection.o \
		.obj/moc_videowidget.o
DIST	   = videowidget.pro
QMAKE_TARGET = videowidget
DESTDIR  = 
TARGET   = videowidget

first: all
####### Implicit rules

.SUFFIXES: .c .o .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: videowidget.pro  /usr/lib/qt-3.3/mkspecs/default/qmake.conf /usr/lib/qt-3.3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile videowidget.pro
qmake: 
	@$(QMAKE) -o Makefile videowidget.pro

dist: 
	@mkdir -p .obj/videowidget && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/videowidget/ && $(COPY_FILE) --parents videowidget.ui.h .obj/videowidget/ && ( cd `dirname .obj/videowidget` && $(TAR) videowidget.tar videowidget && $(GZIP) videowidget.tar ) && $(MOVE) `dirname .obj/videowidget`/videowidget.tar.gz . && $(DEL_FILE) -r .obj/videowidget

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

.obj/main.o: main.cpp .ui/videowidget.h \
		videorenderer.h \
		videoserver.h \
		video.h \
		clientconnection.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.obj/video.o: video.c driver.h
	$(CC) -c $(CFLAGS) $(INCPATH) -o .obj/video.o video.c

.obj/driver.o: driver.c driver.h
	$(CC) -c $(CFLAGS) $(INCPATH) -o .obj/driver.o driver.c

.obj/videorenderer.o: videorenderer.cpp videorenderer.h \
		video.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/videorenderer.o videorenderer.cpp

.obj/videoserver.o: videoserver.cpp videoserver.h \
		clientconnection.h \
		videorenderer.h \
		video.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/videoserver.o videoserver.cpp

.obj/clientconnection.o: clientconnection.cpp clientconnection.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/clientconnection.o clientconnection.cpp

.ui/videowidget.h: videowidget.ui videorenderer.h \
		videoserver.h \
		video.h \
		clientconnection.h
	$(UIC) videowidget.ui -o .ui/videowidget.h

.ui/videowidget.cpp: .ui/videowidget.h videowidget.ui videorenderer.h \
		videoserver.h \
		video.h \
		clientconnection.h
	$(UIC) videowidget.ui -i videowidget.h -o .ui/videowidget.cpp

.obj/videowidget.o: .ui/videowidget.cpp videowidget.ui.h \
		.ui/videowidget.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/videowidget.o .ui/videowidget.cpp

.obj/moc_clientconnection.o: .moc/moc_clientconnection.cpp  clientconnection.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_clientconnection.o .moc/moc_clientconnection.cpp

.obj/moc_videowidget.o: .moc/moc_videowidget.cpp  .ui/videowidget.h videorenderer.h \
		videoserver.h \
		video.h \
		clientconnection.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_videowidget.o .moc/moc_videowidget.cpp

.moc/moc_clientconnection.cpp: $(MOC) clientconnection.h
	$(MOC) clientconnection.h -o .moc/moc_clientconnection.cpp

.moc/moc_videowidget.cpp: $(MOC) .ui/videowidget.h
	$(MOC) .ui/videowidget.h -o .moc/moc_videowidget.cpp

####### Install

install:  

uninstall:  

