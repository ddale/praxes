####### Implicit rules

.SUFFIXES: .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: macchessclient.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile macchessclient.pro
qmake: 
	@$(QMAKE) -o Makefile macchessclient.pro

dist: 
	@mkdir -p .obj/macchessclient && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/macchessclient/ && $(COPY_FILE) --parents mainform.ui.h settingsdialog.ui.h .obj/macchessclient/ && ( cd `dirname .obj/macchessclient` && $(TAR) macchessclient.tar macchessclient && $(GZIP) macchessclient.tar ) && $(MOVE) `dirname .obj/macchessclient`/macchessclient.tar.gz . && $(DEL_FILE) -r .obj/macchessclient

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS) 
		-$(DEL_FILE) .ui/qmake_image_collection.cpp
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

.obj/main.o: main.cpp .ui/mainform.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.obj/videowidget.o: video/.ui/videowidget.cpp video/.ui/videowidget.h \
		video/videowidget.ui.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/videowidget.o video/.ui/videowidget.cpp

.obj/video.o: video/video.c video/xvid.h
	$(CC) -c $(CFLAGS) $(INCPATH) -o .obj/video.o video/video.c

.obj/videorenderer.o: video/videorenderer.cpp video/videorenderer.h \
		datasource.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/videorenderer.o video/videorenderer.cpp

.obj/datasource.o: datasource.cpp datasource.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/datasource.o datasource.cpp

.obj/zfthread.o: zfthread.cpp zfthread.h \
		datasource.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/zfthread.o zfthread.cpp

.ui/mainform.h: mainform.ui datasource.h \
		zfthread.h
	$(UIC) mainform.ui -o .ui/mainform.h

.ui/mainform.cpp: .ui/mainform.h mainform.ui datasource.h \
		zfthread.h
	$(UIC) mainform.ui -i mainform.h -o .ui/mainform.cpp

.ui/settingsdialog.h: settingsdialog.ui 
	$(UIC) settingsdialog.ui -o .ui/settingsdialog.h

.ui/settingsdialog.cpp: .ui/settingsdialog.h settingsdialog.ui 
	$(UIC) settingsdialog.ui -i settingsdialog.h -o .ui/settingsdialog.cpp

.obj/mainform.o: .ui/mainform.cpp .ui/settingsdialog.h \
		mainform.ui.h \
		.ui/mainform.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/mainform.o .ui/mainform.cpp

.obj/settingsdialog.o: .ui/settingsdialog.cpp settingsdialog.ui.h \
		.ui/settingsdialog.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/settingsdialog.o .ui/settingsdialog.cpp

.obj/moc_videowidget.o: .moc/moc_videowidget.cpp video/.ui/videowidget.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_videowidget.o .moc/moc_videowidget.cpp

.obj/moc_mainform.o: .moc/moc_mainform.cpp .ui/mainform.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_mainform.o .moc/moc_mainform.cpp

.obj/moc_settingsdialog.o: .moc/moc_settingsdialog.cpp .ui/settingsdialog.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_settingsdialog.o .moc/moc_settingsdialog.cpp

.moc/moc_videowidget.cpp: $(MOC) video/.ui/videowidget.h
	$(MOC) video/.ui/videowidget.h -o .moc/moc_videowidget.cpp

.moc/moc_mainform.cpp: $(MOC) .ui/mainform.h
	$(MOC) .ui/mainform.h -o .moc/moc_mainform.cpp

.moc/moc_settingsdialog.cpp: $(MOC) .ui/settingsdialog.h
	$(MOC) .ui/settingsdialog.h -o .moc/moc_settingsdialog.cpp

.obj/qmake_image_collection.o: .ui/qmake_image_collection.cpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/qmake_image_collection.o .ui/qmake_image_collection.cpp

.ui/qmake_image_collection.cpp: images/filenew \
		images/fileopen \
		images/filesave \
		images/print \
		images/undo \
		images/redo \
		images/editcut \
		images/editcopy \
		images/editpaste \
